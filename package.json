  "main": "build/backend/app.js",
  "scripts": {
    "build-docs": "graphdoc -s src/common/schema.graphql -o ./dist/docs/graphdoc --force",
    "build-graphql": "node scripts/generate-schema.js && gql-gen",
    "build-front:prod": "webpack --config src/frontend/webpack.config.js --mode production",
    "build-front": "webpack --config src/frontend/webpack.config.js --mode development --progress",
    "build-back": "cd src/backend/ && tsc && ef-tspm && cd ../../",
    "build:prod": "npm run clean && npm run build-back && npm run build-front:prod",
    "build": "npm run clean && npm run build-back && npm run build-front",
    "clean": "rm -rf build dist",
    "dev": "npm-run-all --parallel watch:front watch:back",
    "heroku-postbuild": "npm run build:prod",
    "watch:front": "webpack --config src/frontend/webpack.config.js --watch --mode development --progress",
    "watch:back": "nodemon",
    "start": "node build/backend/app.js"
  },
